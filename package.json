{
  "name": "cnest",
  "version": "1.0.0",
  "description": "New Version Web Framework",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon --ignore 'public/**' --ignore 'public/*'",
    "dev": "npm run dev:server",
    "serve": "node server.js"
},
  "nodemonConfig": {
    "watch": [
      "server.js",
      "endpoint/**/*.js",
      "modules/**/*.js",
      "config/**/*.js"
    ],
    "ext": "js,json",
    "ignore": [
      "public/**/*",
      "public/*",
      "public/src/**/*"
    ]
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^9.4.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.4",
    "@ph7/real-email-validation": "^1.1.2",
    "@supabase/supabase-js": "^2.45.3",
    "axios": "^1.7.7",
    "chokidar": "^3.6.0",
    "compression": "^1.7.4",
    "easy-waf": "^0.6.0",
    "email-domain-check": "^1.1.4",
    "fastify": "^4.28.1",
    "fastify-compress": "^4.1.0",
    "fastify-cookie": "^5.6.1",
    "fastify-socket.io": "^5.1.0",
    "hpp": "^0.2.3",
    "https-proxy-agent": "^7.0.5",
    "inquirer": "^12.0.0",
    "isomorphic-dompurify": "^2.15.0",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.15",
    "timer": "^1.0.1",
    "utf8": "^3.0.0",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "winston": "^3.14.2",
    "xss": "^1.0.15",
    "xss-clean": "^0.1.4"
  },
  "author": "localhost3k AKA radiiplus",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.4"
  }
}
